{
  "name": "solarwinds",
  "version": "0.0.1",
  "description": "The Node.js library and CLI for the Solarwinds API",
  "main": "./lib/solarwinds",
  "bin": {
    "solarwinds": "./lib/cli.js"
  },
  "scripts": {
    "start:dev": "babel-node ./test/playground.js",
    "start:dev:cli": "babel-node ./src/cli.js",
    "build": "babel src/ --out-dir lib/",
    "test": "snyk test && ava ./tests/*.spec.js",
    "test:unit": "ava",
    "snyk:monitor": "snyk auth $SNYK_TOKEN && snyk monitor"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cityofsurrey/solarwinds-node.git"
  },
  "author": "Viatcheslav Ivanov <viatcheslav.ivanov@surrey.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cityofsurrey/solarwinds-node/issues"
  },
  "homepage": "https://github.com/cityofsurrey/solarwinds-node#readme",
  "dependencies": {
    "camelcase-keys": "^3.0.0",
    "commander": "^2.9.0",
    "moment": "^2.13.0",
    "parse-duration": "^0.1.1",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0.5",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "snyk": "^1.16.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "presets": ["es2015", "stage-0"],
    "plugins": ["transform-runtime"]
  }
}
