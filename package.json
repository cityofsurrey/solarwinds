{
  "name": "solarwinds",
  "version": "0.0.1",
  "description": "The Node.js library and CLI for the Solarwinds API",
  "main": "./lib/solarwinds",
  "bin": {
    "solarwinds": "./lib/cli.js"
  },
  "scripts": {
    "dev": "babel-node ./test/playground.js",
    "compile": "babel src/ --out-dir lib/ && sed -i '1 i\\#!/usr/bin/env node' lib/cli.js",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill",
    "publish2npm": "npm run compile && publish"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cityofsurrey/solarwinds-node.git"
  },
  "author": "Viatcheslav Ivanov <viatcheslav.ivanov@surrey.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cityofsurrey/solarwinds-node/issues"
  },
  "homepage": "https://github.com/cityofsurrey/solarwinds-node#readme",
  "dependencies": {
    "request": "^2.72.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2"
  }
}
